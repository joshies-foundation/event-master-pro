{
  "name": "joshies",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "prebuild": "node src/set-last-updated-date.mjs pre",
    "postbuild": "node src/set-last-updated-date.mjs post",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write .",
    "postinstall": "patch-package",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "generate-db-types": "supabase gen types typescript --project-id 'jdwvtjrhfsciqttiueud' --schema public > src/app/shared/util/schema.ts && prettier --write src/app/shared/util/schema.ts"
  },
  "private": true,
  "prettier": {
    "singleQuote": true
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,js,tsx,jsx,css,scss,sass,json,md,yml,yaml,html}": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/router": "^19.2.1",
    "@angular/service-worker": "^19.2.1",
    "@supabase/supabase-js": "^2.44.3",
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "patch-package": "^8.0.0",
    "primeflex": "3.3.1",
    "primeicons": "7.0.0",
    "primeng": "17.18.3",
    "quill": "^1.3.7",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular-eslint/builder": "19.2.0",
    "@angular-eslint/eslint-plugin": "19.2.0",
    "@angular-eslint/eslint-plugin-template": "19.2.0",
    "@angular-eslint/schematics": "19.2.0",
    "@angular-eslint/template-parser": "19.2.0",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@types/jasmine": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "hono": "^4.4.12",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.7",
    "pre-commit": "^1.2.2",
    "prettier": "3.3.2",
    "supabase": "^1.178.2",
    "typescript": "~5.8.2"
  },
  "engines": {
    "node": "22",
    "npm": "10"
  }
}
